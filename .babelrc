{
  "presets": [
    [
      "react"
    ],
    [
      "es2015",
      {
        "modules": false,
        "loose": true
      }
    ],
    [
      "stage-2"
    ]
  ],
  "plugins": [
    [
      "transform-runtime"
    ],
    [
      "transform-object-rest-spread"
    ],
    [
      "transform-es2015-destructuring"
    ]
  ]
}
